#define NUM_PROXIMITY_SENSORS 3



struct DateTime {
	uint16_t Year;
	uint16_t Month;
	uint16_t Day;
	uint16_t Hour;
	uint16_t Minute;
	uint16_t Seconds;
};

struct SensorData {
   uint16_t  Photoresistor;
   struct DateTime DateTime;
   uint16_t Proximity[NUM_PROXIMITY_SENSORS];

} SensorData;


